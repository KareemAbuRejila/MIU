{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "game",
    "type": "object",
    "properties": {
        "designers": {
            "type": "array",
            "items": {
                "type": "string"
            }
        },
        "rate": {
            "type": "number"
        },
        "_id": {
            "type": "string"
        },
        "title": {
            "type": "string"
        },
        "year": {
            "type": "number"
        },
        "price": {
            "type": "number"
        },
        "minPlayers": {
            "type": "number"
        },
        "maxPlayers": {
            "type": "number"
        },
        "publisher": {
            "type": "object",
            "properties": {
                "location": {
                    "type": "object",
                    "properties": {
                        "coordinates": {
                            "type": "array",
                            "items": {
                                "type": "number"
                            }
                        }
                    }
                },
                "_id": {
                    "type": "string"
                },
                "name": {
                    "type": "string"
                }
            }
        },
        "reviews": {
            "type": "array",
            "items": {
                "type": "string"
            }
        },
        "minAge": {
            "type": "number"
        }
    }
}